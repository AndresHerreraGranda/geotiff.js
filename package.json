{
  "name": "geotiff",
  "version": "0.4.1",
  "description": "GeoTIFF image decoding in JavaScript",
  "repository": "https://github.com/constantinius/geotiff.js",
  "keywords": [
    "TIFF",
    "GeoTIFF",
    "image",
    "raster"
  ],
  "main": "dist/main.js",
  "dependencies": {
    "pako": "^1.0.3",
    "webworkify": "^1.4.0",
    "xmldom": "0.1.*"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-eslint": "^7.1.1",
    "babel-loader": "^7.1.2",
    "babel-plugin-syntax-async-functions": "^6.13.0",
    "babel-plugin-transform-async-to-generator": "^6.16.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-env": "^1.6.1",
    "chai": "^3.5.0",
    "detect-node": "^2.0.3",
    "es6-promise": "^4.0.5",
    "eslint": "^4.18.0",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-react": "^7.7.0",
    "isomorphic-fetch": "^2.2.1",
    "jsdoc-babel": "^0.2.1",
    "jshint-stylish": "^2.2.1",
    "mocha": "^3.1.2",
    "webpack": "^3.10.0",
    "webpack-dev-server": "^2.11.1"
  },
  "scripts": {
    "dev": "webpack-dev-server -w --entry ./src/main --output-filename dist/geotiff.bundle.js --devtool eval-cheap-module-source-map",
    "watch": "webpack -w --entry ./src/main --output-filename dist/geotiff.bundle.js --devtool eval-cheap-module-source-map",
    "build": "webpack --entry ./src/main --output-filename dist/geotiff.bundle.js --devtool eval-cheap-module-source-map",
    "test": "mocha --compilers js:babel-core/register",
    "lint": "eslint src",
    "docs": "rm -rf docs/*; jsdoc -c .jsdoc.json -r src README.md -d docs",
    "prepublish": "npm run build"
  },
  "author": "Fabian Schindler",
  "contributors": [
    {
      "name": "Fabian Schindler",
      "email": "fabian.schindler@eox.at"
    }
  ],
  "license": "MIT"
}
